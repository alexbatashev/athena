<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <Configurations>Release | macOS;Debug | macOS;Debug macOS;Release macOS;Debug GTK;Release GTK;Debug WPF;Release WPF</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AutomaticGraphLayout" Version="1.1.6" />
        <PackageReference Include="AutomaticGraphLayout.Drawing" Version="1.1.6" />
        <PackageReference Include="Google.Protobuf" Version="3.11.4" />
        <PackageReference Include="Grpc.Tools" Version="2.28.0-pre3" PrivateAssets="All" />
        <PackageReference Include="Svg" Version="3.0.102" />
    </ItemGroup>

    <ItemGroup>
        <Protobuf Include="../../graph_proto/*.proto" />
    </ItemGroup>

</Project>
